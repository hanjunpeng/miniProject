/**index.wxss**/
.container{
  width: 100%;
  height: 100%;
  background-color: #ededed;
  font-size: 16px;
  font-family: -apple-system-font,Helvetica Neue,Helvetica,sans-serif;
}
.container-bd{
  padding: 0 15px 40px 15px;
}
.container-box{
  margin-bottom: 8px;
  background-color: #fff;
  border-radius: 5px;
}
.container-li{
  overflow: hidden;
  font-size: 12px;
  display: flex;
  align-items: center;
  justify-content: space-between;
}
.container-flex{
  padding: 20px;
  flex: 1;
}
.container-iconcommon{
  margin-right: 10px;
}
.container-icon{
  display: inline-block;
  height: 8px;
  width: 8px;
  border-width: 2px 2px 0 0;
  border-color: #B2B2B2;
  border-style: solid;
  transform: rotate(45deg);
}
.navigator-hover{
  background-color: #fff;
}
.container-iconXia{
  
}
.container-selecticon{
  opacity: 0.8;
}
.container-selecticon .container-icon{
  transform: rotate(135deg);
}
.container-kind{
  height: 0;
  overflow: hidden;
}
.container-list-show{
  height: auto;
}
.container-cells .container-navi:last-child{
  border-bottom: 0;
}
.container-navi{
  display: flex;
  align-items: center;
  justify-content: space-between;
  border-bottom: 1px solid rgba(0, 0, 0, 0.1);
  margin-right: 12px;
  margin-left: 20px;
  padding-top: 15px;
  padding-bottom: 10px;
}
.containertap{
  padding: 10px;
  background: #fff;
}